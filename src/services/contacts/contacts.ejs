<html>
  <head>
    <style>
      table {
        border-collapse: collapse;
        width: 100%;
      }
      th, td {
        border: 1px solid #dddddd;
        text-align: left;
        padding: 8px;
      }
      tr:nth-child(even) {
        background-color: #f2f2f2;
      }
    </style>
  </head>
  <body style="font-family: Arial, Helvetica, sans-serif;">
    <h1>Contatos</h1>
    <table>
      <thead>
        <tr>
          <th>Nome</th>
          <th>Email</th>
          <th>Telefone</th>
          <th>Nome do Cliente</th>
          <th>ID do Cliente</th>
        </tr>
      </thead>
      <tbody>
        <% contacts.forEach(function(contact) { %>
          <tr>
            <td><%= contact.name %></td>
            <td><%= contact.email %></td>
            <td><%= contact.phone %></td>
            <td><%= contact.client.name %></td>
            <td><%= contact.client.id %></td>
          </tr>
        <% }); %>
      </tbody>
    </table>
  </body>
</html>
